    "xstate": "^4.32.1"
  },
  "scripts": {
    "build": "export VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) && yarn build:gqty && yarn build:clean && yarn build:spago && yarn build:next",
    "build:clean": "rm -Rf output",
    "build:spago": "spago -x spago.production.dhall build",
    "build:next": "next build",
    "build:gqty": "gqty generate",
    "clean": "rm -Rf .cache .next output output-es node_modules .spago *.lock",
    "dev": "export VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) && yarn dev:clean && yarn dev:build && yarn dev:serve",
    "dev:clean": "rm -Rf output",
    "dev:build": "spago build",
    "dev:serve": "spago build --watch & next",
    "postinstall": "spago install",
    "start": "export VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) && yarn build && next start",
    "lint": "next lint"
  },
  "devDependencies": {
    "@gqty/cli": "^3.3.0",
    "@svgr/webpack": "^6.3.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^17.0.23",
    "@types/papaparse": "^5.3.2",
    "@types/platform": "^1.3.4",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "encoding": "^0.1.13",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.4",
    "eslint-plugin-functional": "^4.2.1",
    "file-loader": "^6.2.0",
    "purescript": "0.15.6",
    "purs-backend-es": "^1.3.1",
    "spago": "^0.20.9",
    "tsutils": "^3.21.0",
    "typescript": "4.7.4"
  },
  "gqty": {
    "react": true,
    "scalarTypes": {
      "DateTime": "string"
    },
    "introspection": {
      "endpoint": "core/gql/schema.gql"
    },
    "destination": "./core/gql/index.ts",
    "javascriptOutput": false,
    "subscriptions": true
  }
}

    "jwt-decode": "^3.1.2",
    "next": "^13.1.6",
    "nextjs-redirect": "^6.0.0",
    "papaparse": "^5.3.2",
    "platform": "^1.3.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.4",
    "sha1": "^1.1.1",
    "slate": "^0.80.0",
    "slate-history": "^0.66.0",
    "slate-hyperscript": "^0.77.0",
